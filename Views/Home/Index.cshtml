@model IEnumerable<Employee>
<html>
<head>
    <title>Employee Index</title>
</head>
<body>
    <h1>Employee List</h1>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Department</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var emp in Model)
            {
                <tr>
                    <td>@emp?.Id </td>
                    <td> @emp?.Name </td>
                    <td> @emp?.Department </td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>